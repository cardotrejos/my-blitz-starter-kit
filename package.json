    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hanger": "^2.4.5",
    "resend": "^1.0.0",
    "secure-password": "4.0.0",
    "uploadthing": "^6.3.3",
    "zod": "^3.22.4",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.8",
